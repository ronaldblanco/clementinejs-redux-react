{
  "name": "clementinejs-react-redux-pnald",
  "version": "1.0.2",
  "engines": {
    "node": "8.9.1"
  },
  "description": "A full-stack app boilerplate for the freeCodeCamp Course.",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ronaldblanco/clementinejs-redux-react"
  },
  "bugs": "https://github.com/ronaldblanco/clementinejs-redux-react/issues",
  "keywords": [
    "MongoDB",
    "Mongoose",
    "full stack",
    "Express",
    "Node",
    "javascript",
    "React",
    "boilerplate",
    "webpack",
    "Redux",
    "react-redux",
    "redux-form"
  ],
  "scripts": {
    "start": "nodemon dist/server.js --exec babel-node -e txt",
    "start-pm2": "pm2 start dist/server.js",
    "production": "nodemon dist/server.js --exec babel-node -e txt",
    "development": "nodemon server.js --exec babel-node --ignore 'dist/**/*.*' -e js,jsx,json",
    "development-sync": "nodemon server.js --exec babel-node --ignore 'dist/**/*.*' -e js,jsx,json --trace-sync-io",
    "postinstall": "make webpack",
    "build": "make build",
    "build-dev": "make build_dev",
    "win-postinstall": "postinstall.cmd",
    "win-build": "production.cmd",
    "win-build-dev": "development.cmd",
    "lint": "eslint --ext .js --ext .jsx ./app ./client/src",
    "test": "npm run test:server:once && npm run test:client:once",
    "test:client": "npm run test:client:once -- --watch",
    "test:server": "npm run test:server:once -- --watch",
    "test:client:once": "cross-env NODE_ENV=test mocha --require babel-register --require ignore-styles --require client/entry.test.js \"client/**/*.spec.js\"",
    "test:server:once": "cross-env NODE_ENV=test mocha --timeout 5000 --require babel-register --require app/entry.test.js \"app/**/*.spec.js\""
  },
  "author": "Ronald Blanco <pnald.blanco@gmail.com> (https://github.com/ronaldblanco)",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.3.13",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-enzyme": "^0.8.0",
    "cheerio": "^0.19.0",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "cross-env": "^5.1.1",
    "css-loader": "^0.23.1",
    "dotenv": "^1.2.0",
    "emailjs": "^1.0.12",
    "enzyme": "^1.6.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-yields": "^1.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "helmet": "^3.9.0",
    "http": "0.0.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "md5-hex": "^2.0.0",
    "mocha": "^4.0.1",
    "mongoose": "^4.13.7",
    "mongoose-auto-increment": "^5.0.1",
    "node-sass": "^3.4.2",
    "nodemon": "^1.12.7",
    "passport": "^0.3.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "prop-types": "^15.6.0",
    "randomatic": "^2.0.0",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.4.1",
    "redux": "^3.3.0",
    "redux-form": "^7.2.0",
    "redux-form-website-template": "0.0.80",
    "redux-thunk": "^1.0.3",
    "resolve-url": "^0.2.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^3.1.2",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "socket.io": "^2.0.4",
    "style-loader": "^0.13.0",
    "supertest": "^3.0.0",
    "urlparser": "^0.3.9",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-node-externals": "^1.2.0",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2"
  },
  "devDependencies": {}
}
